<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="No Users Found"
		title="Select User"
		search=".onUserSearch"
		cancel=".onUserDialogClose"
		items="{
			path: '/Users'
		}">
		<StandardListItem
			title="{LastName}, {FirstName}"
			description="{EmailID}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>